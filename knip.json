{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": [
		"src/main.tsx",
		"src/root.tsx",
		"src/routes/**/*.tsx",
		"src/scripts/*.ts",
		"src/entry.worker.ts"
	],
	"project": ["src/**/*.{ts,tsx}"],
	"ignore": [
		"src/routes/**/+types/**",
		"src/db/migrations/**",
		"src/db/schema.ts",
		"src/components/ui/**",
		"src/scripts/seed-data/**"
	],
	"ignoreDependencies": [
		"@vitejs/plugin-react",
		"@react-router/cloudflare",
		"@react-router/node"
	],
	"ignoreExportsUsedInFile": true
}
