{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": [
		"src/main.tsx",
		"src/root.tsx",
		"src/routes/**/*.tsx",
		"src/scripts/*.ts"
	],
	"project": ["src/**/*.{ts,tsx}"],
	"ignore": [
		"src/routes/**/+types/**",
		"src/db/migrations/**",
		"src/db/schema.ts",
		"src/components/ui/**"
	],
	"ignoreDependencies": ["@vitejs/plugin-react"],
	"ignoreExportsUsedInFile": true
}
